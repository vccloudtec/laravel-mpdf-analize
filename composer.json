{
    "name": "wendel-ulhoa/laravel-mpdf-analize",
    "description": "Laravel Mpdf: Using Mpdf in Laravel to generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "mpdf/mpdf": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wendelulhoa\\LaravelMpdf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wendelulhoa\\LaravelMpdf\\LaravelMpdfServiceProvider"
            ],
            "aliases": {
                "PDF": "Wendelulhoa\\LaravelMpdf\\Facades\\LaravelMpdf"
            }
        }
    }
}
